{
  "name": "fx-electron",
  "version": "0.0.1",
  "description": "FX App using Electron",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "clean": "rm -rf build dist",
    "ts:build": "tsc -b",
    "ts:watch": "tsc -w",
    "dev": "webpack --config webpack.config.dev.js"
  },
  "keywords": [
    "fx",
    "electron"
  ],
  "author": "Cy2code",
  "license": "MIT",
  "main": "build/app.js",
  "build": {
    "appId": "com.cy2code.fx",
    "files": [
      "node_modules/**/*",
      "build/**/*",
      "package.json"
    ],
    "extends": null
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue-material": "^1.0.0-beta-14",
    "vuelidate": "^0.7.5",
    "vuex": "^3.3.0",
    "vuex-electron": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/vuelidate": "^0.7.13",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "astroturf": "^0.10.4",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.11.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^3.8.3",
    "v8-compile-cache": "^2.1.1",
    "vue-devtools": "^5.1.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
